<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="minh.assignment1.quizzgame">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_quizz_game"
        android:label="@string/app_name"

        android:supportsRtl="true"
        android:theme="@style/Theme.QuizzGame">
        <activity android:name=".MainActivity"/>
        <activity android:name=".GameOverActivity"/>
        <activity android:name=".EsportQuestionsActivity"/>
        <activity android:name=".IntroductionActivity"/>
        <activity
            android:name=".SplashWelcomeActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>